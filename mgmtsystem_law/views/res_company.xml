<odoo>
    <record id="view_company_form" model="ir.ui.view">
        <field name="name">view_company_form_rss_url</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[@colspan='4']/page[@name='general_info']" position="after">
                <page string="Law RSS" name="law_rss">
                    <group>
                        <group>
                            <field name="law_rss_url" string="RSS URL"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
